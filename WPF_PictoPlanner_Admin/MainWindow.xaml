<Window x:Class="WPF_PictoPlanner_Admin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_PictoPlanner_Admin"
        xmlns:viewmodels="clr-namespace:WPF_PictoPlanner_Admin.ViewModels"
        xmlns:views="clr-namespace:WPF_PictoPlanner_Admin.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <DataTemplate x:Name="usersViewTemplate" DataType="{x:Type viewmodels:UsersViewModel}">
            <views:Users/>
        </DataTemplate>
    </Window.Resources>

    <ContentControl Content="{Binding}"></ContentControl>
</Window>
    
